<section class="section-wrapper personal-project-section gray-bg">
	<div class="container-fluid">
		<div class="row">
			<div class="col-md-12">
				<div class="section-title">
					<h2>Personal Projects</h2>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12">
				<% work.list.forEach(function(item) { %>
					<div class="content-item">
						<h3><%=item.name%>&nbsp;&nbsp;<a class="fa fa-external-link" href="<%=item.link%>" target="_blank"></a></h3>
						<p><%=item.desc%></p>
					</div>
				<% })%>
			</div>
		</div>
	</div>
</section>